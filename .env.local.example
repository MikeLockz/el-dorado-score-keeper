# Copy to .env.local for local development.
# Provide sandbox credentials supplied by New Relic.
NEXT_PUBLIC_OBSERVABILITY_ENABLED=false
# Supported providers: newrelic | posthog | custom
NEXT_PUBLIC_OBSERVABILITY_PROVIDER=newrelic
NEXT_PUBLIC_NEW_RELIC_LICENSE_KEY=
# Defaults to the production ingestion host; set to http://localhost:5050 when using the proxy.
NEXT_PUBLIC_NEW_RELIC_BROWSER_HOST=https://log-api.newrelic.com
NEXT_PUBLIC_APP_ENV=development
NEXT_PUBLIC_NEW_RELIC_BROWSER_SERVICE_NAME=el-dorado-score-keeper-web

# Optional: configure the Browser agent directly
NEXT_PUBLIC_NEW_RELIC_APP_ID=
NEXT_PUBLIC_NEW_RELIC_BROWSER_SCRIPT_URL=https://js-agent.newrelic.com/nr-loader-spa-current.min.js # optional override, defaults to this value
NEXT_PUBLIC_NEW_RELIC_BROWSER_LICENSE_KEY=
NEXT_PUBLIC_NEW_RELIC_ALLOW_DEV_AGENT=false # set to true when running the proxy locally
# When enabling the agent locally, point the browser + beacon hosts to the proxy to avoid CORS
# NEXT_PUBLIC_NEW_RELIC_BROWSER_HOST=http://localhost:5050
# NEXT_PUBLIC_NEW_RELIC_BROWSER_BEACON=http://localhost:5050
# NEXT_PUBLIC_NEW_RELIC_BROWSER_ERROR_BEACON=http://localhost:5050
# (scheme is optional; we strip it for the beacon fields if provided)

# PostHog (when NEXT_PUBLIC_OBSERVABILITY_PROVIDER=posthog)
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com
NEXT_PUBLIC_POSTHOG_DEBUG=false

# Legacy HyperDX env vars remain supported but trigger dev warnings.
